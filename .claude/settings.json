{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(python *)",
      "Bash(npm run *)",
      "Bash(npx openclaw *)"
    ],
    "deny": [
      "Read(./.env)",
      "Read(./.env.*)",
      "Read(./.github_config)",
      "Read(./**/*.token)"
    ]
  },
  "env": {}
}
